# All Target
all: listen_server_poolled.elf secondary-outputs

# Tool invocations
listen_server_poolled.elf: $(OBJS) ../src/lscript.ld $(USER_OBJS)
	@echo Building target: $@
	@echo Invoking: MicroBlaze gcc linker
	mb-gcc -Wl,-T -Wl,../src/lscript.ld -L../../standalone_bsp/microblaze_0/lib -mlittle-endian -mxl-barrel-shift -mxl-pattern-compare -mno-xl-soft-div -mcpu=v8.20.a -mno-xl-soft-mul -mxl-multiply-high -mhard-float -mxl-float-convert -mxl-float-sqrt -o"listen_server_poolled.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo Finished building target: $@
	@echo ' '

listen_server_poolled.elf.size: listen_server_poolled.elf
	@echo Invoking: MicroBlaze Print Size
	mb-size listen_server_poolled.elf  |tee "listen_server_poolled.elf.size"
	@echo Finished building: $@
	@echo ' '

listen_server_poolled.elf.elfcheck: listen_server_poolled.elf
	@echo Invoking: Xilinx ELF Check
	elfcheck listen_server_poolled.elf -hw ../../hw_platform/system.xml -pe microblaze_0  |tee "listen_server_poolled.elf.elfcheck"
	@echo Finished building: $@
	@echo ' '

# Other Targets
